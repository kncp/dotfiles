alias ls='exa'
alias ll='exa -1'
alias la='exa -aghHl'
alias cat='ccat'
alias tree='exa -T'
alias vi='nvim'
alias diff='colordiff -u'
alias top='htop'

# https://github.com/remind101/assume-role
alias assume-role='function(){eval $(command assume-role $@);}'

# reload zsh
alias reload='exec zsh -l'
